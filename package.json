{
  "name": "kbd",
  "version": "0.1.0",
  "description": "A tone.js keyboard",
  "scripts": {
    "tsc:w": "tsc -w",
    "wdm": "webpack-dev-server --hot --inline --mode development",
    "build": "tsc ; webpack --progress -p",
    "server": "concurrently \"npm run tsc:w\" \"npm run wdm\""
  },
  "author": "Сухарик <suhr@i2pmail.org>",
  "license": "MIT",
  "dependencies": {
    "tone": "^13.8.25"
  },
  "devDependencies": {
    "@types/tone": "git://github.com/Tonejs/TypeScript.git",
    "concurrently": "^4.1.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3"
  }
}
